			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="kval_zivo_skoln_hodn_kod" type="dsComm:str10" use="required">
			<xs:annotation>
				<xs:documentation>Kvalita života <cl>SkolniHodnoceni</cl>
